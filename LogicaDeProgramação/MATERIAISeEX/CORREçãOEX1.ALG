Algoritmo "CorreçãoEX1"

Var
numero, antecessor: inteiro


Inicio
// ENTRADA
//numero
escreval ("Digite um número:")
leia(numero)
limpatela ()

//PROCESSAMENTO
// numero -1
antecessor <- numero -1
limpatela ()

//SAIDA
// escrever no terminal antecessor
escreval ("O antecessor do número é ", numero," é de ",antecessor)

Fimalgoritmo